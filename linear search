#include<stdio.h>
void main()
{
    int a[20],key,n,flag=0,i;
    printf("enter array limit\n");
    scanf("%d",&n);
    printf("enter %d elements \n",n);
    for(i=0;i<n;i++)
    scanf("%d",&a[i]);
    printf("enter the number to be searched\n");
    scanf("%d",&key);
    for(i=0;i<n;i++)
    if(a[i]==key)
    {
        flag=1;
        break;
    }
    if(flag)
    printf("\n search successful\n");
    else
    printf("\n search failed\n");
}
